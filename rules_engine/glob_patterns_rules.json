{
	"glob_patterns_regex": {   ";.+":"*",
							   "^/tmp/.+":"/tmp/**",
							   "^/proc/self":"/proc/*",
							   "^/proc/[0-9]+":"/proc/*",
							   "^/proc/.+/task/[0-9]+/":"/proc/*/task/*/",
							   "^/sys/fs/aufs/si_[a-z0-9]+/br[0-9]+":"/sys/fs/aufs/si_*/br*",
							   "^/sys/fs/aufs/si_[a-z0-9]+/$":"/sys/fs/aufs/si_*/",
							   "^/sys/kernel/slab/[^/]+":"/sys/kernel/slab/*",
							   "/usr/share/.+":"/usr/share/**",
							   "/root/.gnupg/.+":"/root/.gnupg/**",
							   "/root/.cache/.+":"/root/.cache/**",
							   "/var/tmp/.+":"/var/tmp/**",
							   "/var/log/.+":"/var/log/**",
							   "/var/cache/.+":"/var/cache/**",
							   "/etc/shadow\\.[0-9]+":"/etc/shadow.*",
							   "/etc/gshadow\\.[0-9]+":"/etc/gshadow.*",
							   "/etc/group\\.[0-9]+":"/etc/group.*",
							   "/etc/passwd\\.[0-9]+":"/etc/passwd.*",
							   "/etc/subuid\\.[0-9]+":"/etc/subuid.*",
							   "/etc/sed[^/]+":"/etc/sed*",
							   "/\\.pivot_root[0-9]+" :"/.pivot_root*",
							   "/docker-aufs-union[0-9]+":"/docker-aufs-union*",
							   "/\\.wh\\.\\.wh\\.[^/]+":"/.wh..wh.*",
							   "/\\.wh\\.\\.wh\\..+/.+":"/.wh..wh.*/**"
						   	},
	"container_id":"[a-z0-9]{64}",
	"container_root":"/var/lib/docker/aufs/diff/*",
	"container_home":"/var/lib/docker",
	"container_full_access_paths":["/tmp/", "/root/", "/data/", "/usr/src/", "/var/lib/mysql/", "/dev/shm/",
					               "/etc/ssl/", "/etc/apt/", "/etc/mysql/", "/etc/postgresql-common/",
					               "/usr/include/curl/", "/var/lib/dpkg/"],
	"container_build_full_access_paths":["/usr/", "/var/lib/", "/etc/ssh/"]
}